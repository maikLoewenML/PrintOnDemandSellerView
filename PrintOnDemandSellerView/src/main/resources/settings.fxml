<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.printondemandsellerview.JavaFXSettingsController">
   <children>
      <Label layoutX="30.0" layoutY="130.0" prefHeight="23.0" prefWidth="130.0" text="MBA">
         <font>
            <Font name="Calibri Bold" size="20.0" />
         </font>
      </Label>
      <TextField fx:id="emailAmazonMerchOnDemand" layoutX="181.0" layoutY="130.0" promptText="E-Mail" />
      <PasswordField fx:id="passwordAmazonMerchOnDemand" layoutX="181.0" layoutY="164.0" promptText="Password" />
      <Button layoutX="414.0" layoutY="165.0" mnemonicParsing="false" onMouseClicked="#saveLogins" prefHeight="58.0" prefWidth="149.0" text="Save">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Label fx:id="settingsLabel" text="settingsLabel" visible="false" />
      <Button layoutX="-1.0" layoutY="-4.0" mnemonicParsing="false" onMouseClicked="#openStartSceneFromSettingsScene" prefHeight="25.0" prefWidth="91.0" text="Start" />
      <Button layoutX="90.0" layoutY="-4.0" mnemonicParsing="false" onMouseClicked="#openSellerViewSceneFromSettingsScene" prefHeight="25.0" prefWidth="91.0" text="Seller View" />
      <Label layoutX="31.0" layoutY="210.0" prefHeight="23.0" prefWidth="130.0" text="Spreadshirt">
         <font>
            <Font name="Calibri Bold" size="20.0" />
         </font>
      </Label>
      <TextField fx:id="emailSpreadshirt" layoutX="182.0" layoutY="210.0" promptText="E-Mail" />
      <PasswordField fx:id="passwordSpreadshirt" layoutX="182.0" layoutY="244.0" promptText="Password" />
   </children>
</AnchorPane>
